server {
    listen 5050 default_server;
    listen [::]:5050 default_server;
    root /var/www/html/wordpress;
#    root ./;
    index index.php;
    server_name localhost;
    location / {
        try_files $uri $uri/ /index.php;
    }
    location ~ \.php$ {
        fastcgi_pass      127.0.0.1:9000;
        fastcgi_index     index.php;
        include           /etc/nginx/fastcgi.conf;
    }
}